{
  "name": "@najit-najist/api",
  "type": "module",
  "version": "0.0.1",
  "types": "./dist/index.d.ts",
  "main": "dist/index.js",
  "exports": {
    "./*": "./*",
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "ondrej-langr <langr.ondrej.work@gmail.com>",
  "private": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:package\" \"npm:dev:package:types\"",
    "dev:server": "vite-node -w src/server.ts",
    "dev:package": "npm run build:package -- --watch",
    "dev:package:types": "npm run build:types -- --watch",
    "prebuild": "rimraf dist",
    "build": "npm run build:package && npm run build:types",
    "build:package": "swc ./src -d dist",
    "build:types": "tsc",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@najit-najist/pb": "*",
    "@najit-najist/tsconfig": "*",
    "@swc/core": "^1.3.41",
    "@types/bcrypt": "^5.0.0",
    "@types/ejs": "^3.1.1",
    "@types/email-templates": "^10.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.3",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-local": "^1.0.34",
    "@types/zxcvbn": "^4.4.1",
    "dotenv-cli": "^6.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-node": "^0.29.2",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "dependencies": {
    "@faker-js/faker": "^7.3.0",
    "@fastify/auth": "^4.1.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.1.0",
    "@fastify/jwt": "^6.3.2",
    "@fastify/passport": "^2.2.0",
    "@fastify/rate-limit": "^7.5.0",
    "@fastify/session": "^10.0.2",
    "@najit-najist/pb": "*",
    "@trpc/server": "^10.14.0",
    "bcrypt": "^5.1.0",
    "concurrently": "^7.5.0",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "email-templates": "^10.0.1",
    "fastify": "^4.9.2",
    "fastify-plugin": "^4.3.0",
    "fs-extra": "^10.1.0",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pino-pretty": "^9.1.1",
    "superjson": "^1.11.0",
    "vitest": "^0.24.5",
    "zod": "^3.19.1",
    "zod-to-json-schema": "^3.18.1",
    "zxcvbn": "^4.4.2"
  }
}
