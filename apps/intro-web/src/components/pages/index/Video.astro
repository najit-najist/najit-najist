---
import Title from '../../Title.astro';
import ourStoryUrl from '../../../assets/our-story.mp4';
---

<div class="max-w-5xl px-4 mx-auto">
  <div class="mx-auto text-center">
    <Title>Náš příběh</Title>
  </div>
  <div class="sm:w-[85%] mx-auto">
    <video controls width="100%" class="sm:aspect-video bg-black">
      <source src={ourStoryUrl} type="video/mp4" />
    </video>
  </div>
</div>
