{
  "extends": "@najit-najist/tsconfig/base.json",
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "preserve",
    "rootDir": "./src",
    "outDir": "./dist",
  },
  "tsc-alias": {
    "resolveFullPaths": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
