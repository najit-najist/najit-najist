{
  "name": "najit-jist",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:packages": "turbo run build --filter=./packages/*",
    "build:web": "turbo run build --filter=@najit-najist/web",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "lint": "turbo run lint",
    "start-production": "npm run migration:migrate --workspace=@najit-najist/database && npm run start --workspace=@najit-najist/web",
    "reinstall": "npm run uninstall && npm install",
    "test": "turbo run test --no-daemon",
    "uninstall": "rm -rf ./**/node_modules ./package-lock.json"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.96",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.4.0",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.5.5",
    "prettier-plugin-packagejson": "^2.4.5",
    "rimraf": "^3.0.2",
    "turbo": "latest",
    "typescript": "^5.2.2"
  },
  "packageManager": "npm@8.19.2",
  "engines": {
    "node": ">=14.0.0"
  },
  "overrides": {
    "next": "^14.0.1"
  }
}
